-- corner
local corner = c("UICorner")
corner.CornerRadius = UDim.new(0, 10)
corner.Parent = scrollingFrame
local corner2 = c("UICorner")
corner2.CornerRadius = UDim.new(0, 10)
corner2.Parent = pic
local corner3 = c("UICorner")
corner3.CornerRadius = UDim.new(0, 10)
corner3.Parent = sadness
--
local closebutton = c("TextButton")
closebutton.Name = "close"
closebutton.Text = "X"
closebutton.TextSize = 15
closebutton.Size = UDim2.new(0.03, 0, 0.07, 0)
closebutton.Position = po(0.73, 0, 0.11)
closebutton.Parent = mainbook
closebutton.TextColor3 = color
closebutton.BackgroundTransparency = 0
closebutton.MouseButton1Click:Connect(function()
mainbook:Destroy()
end)
-- most important buttons here
local slapsec = c("TextButton")
slapsec.Name = "slapsection"
slapsec.Text = "SLAP"
slapsec.Size = UDim2.new(0.04, 0, 0.1, 0)
slapsec.Position = po(0.72, 0, 0.25)
slapsec.Parent = mainbook
-- badge section
local badge = c("TextButton")
badge.Name = "badgesection"
badge.Text = "BADGE"
badge.Size = UDim2.new(0.04, 0, 0.1, 0)
badge.Position = po(0.72, 0, 0.38)
badge.TextSize = 6
badge.Parent = mainbook
-- border
slapsec.BorderSizePixel = 3
slapsec.BorderColor3 = Color3.new(255, 255, 255)
--another frame
local fuck = c("ScrollingFrame")
fuck.BackgroundTransparency = 1
fuck.Name = "badgegloves"
fuck.Size = UDim2.new(0.5, 0, 0.8, 0)
scrollingFrame2.Position = UDim2.new(0.2, 0, 0.1)
fuck.Active = true
fuck.Parent = mainbook
local corner4 = c('UICorner')
corner4.CornerRadius = UDim.new(0, 10)
corner4.Parent = scrollingFrame2
fuck.Visible = false
-- rest from now
badge.MouseButton1Click:Connect(function()
  badge.BorderSizePixel = 3
  wait()
  badge.BorderColor3 = Color3.new(255, 255, 255)
  wait()
  slapsec.BorderSizePixel = 0
  slapsec.BorderColor3 = Color3.new(0, 0, 0)
  wait()
  scrollingFrame.Visible = false
  wait()
  fuck.Visible = true
end)
slapsec.MouseButton1Click:Connect(function()
slapsec.BorderSizePixel = 3
slapsec.BorderColor3 = Color3.new(255, 255, 255)
wait()
badge.BorderSizePixel = 0
badge.BorderColor3 = Color3.new(0, 0, 0)
wait()
scrollingFrame.Visible = true
fuck.Visible = false
end)
