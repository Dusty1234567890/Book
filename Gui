local function CustomSendNotification(title, text, icon)
    local gui = Instance.new("ScreenGui")
    gui.Name = "NotificationGui"
    gui.Parent = game.Players.LocalPlayer.PlayerGui
    local FRAME = Instance.new("Frame")
    FRAME.Size = UDim2.new(0.4, 0, 0.2, 0)
    FRAME.Position = UDim2.new(0.7, 0, 0.7, 0)
    FRAME.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    FRAME.BackgroundTransparency = 0
    FRAME.Parent = gui
    local titleTextLabel = Instance.new("TextLabel")
    titleTextLabel.Size = UDim2.new(0.3, 0, 0.1, 0)
    titleTextLabel.Position = UDim2.new(-0.09, 0, 0.1, 0)
    titleTextLabel.Text = title
    titleTextLabel.Font = Enum.Font.AmaticSC
    titleTextLabel.TextSize = 35
    titleTextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    titleTextLabel.BackgroundTransparency = 1
    titleTextLabel.Parent = FRAME
    local textTextLabel = Instance.new("TextLabel")
    textTextLabel.Size = UDim2.new(1, 0, 0.5, 0)
    textTextLabel.Position = UDim2.new(-0.03, 0, 0.3, 0)
    textTextLabel.Text = text
    textTextLabel.Font = Enum.Font.AmaticSC
    textTextLabel.TextSize = 30
    textTextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    textTextLabel.BackgroundTransparency = 1
    textTextLabel.Parent = FRAME
    if icon then
        local iconImageLabel = Instance.new("ImageLabel")
        iconImageLabel.Size = UDim2.new(0.2, 0, 0.7, 0)
        iconImageLabel.Position = UDim2.new(0, 0, 0.32, 0)
        iconImageLabel.Image = icon
iconImageLabel.BackgroundTransparency = 1
        iconImageLabel.Parent = FRAME
    end
    wait(3)
    FRAME.Transparency = 0.2
    wait()
    FRAME.Transparency = 0.4
    wait()
    FRAME.Transparency = 0.6
    wait()
    FRAME.Transparency = 0.8
    wait()
    FRAME.Transparency = 1
    wait()
    gui:Destroy()
end
