-- main part
button.MouseButton1Click:Connect(function()
local lobby = game.Players.LocalPlayer.Character:FindFirstChild("InLobby")
local slaps = game.Players.LocalPlayer.leaderstats.Slaps
if lobby then
    if slaps.Value >= 0 then
fireclickdetector(game.workspace.Lobby.Default.ClickDetector)
    else
CustomSendNotification("info", "you dont own this glove", "rbxassetid://17105411523")
    end
elseif lobby == nil then
CustomSendNotification("info", "you're not in lobby", "rbxassetid://17105411523")
end
end)
